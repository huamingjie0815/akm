{
  "name": "apikey-manage",
  "version": "1.0.0",
  "description": "CLI tool to manage LLM API profiles - store and manage base URLs, providers, models, and API keys",
  "main": "dist/index.js",
  "bin": {
    "apikey-manage": "dist/index.js",
    "akm": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cli",
    "llm",
    "api",
    "key",
    "profile",
    "openai",
    "anthropic",
    "management"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "chalk": "^4.1.2",
    "commander": "^14.0.3",
    "inquirer": "^9.2.12",
    "ora": "^5.4.1",
    "zod": "^4.3.6"
  }
}
